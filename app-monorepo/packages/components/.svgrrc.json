{
  "native": true,
  "typescript": true,
  "jsxRuntime": "automatic",
  "svgoConfig": {
    "plugins": [
      "removeDimensions",
      "removeXMLNS",
      {
        "name": "preset-default",
        "params": {
          "overrides": {
            "cleanupIds": false,
            "removeUselessDefs": false
          }
        }
      },
      {
        "name": "addAttributesToSVGElement",
        "params": {
          "attributes": [
            {
              "accessibilityRole": "image"
            }
          ]
        }
      },
      {
        "name": "removeAttrs",
        "params": {
          "attrs": "aria-hidden"
        }
      }
    ]
  }
}
